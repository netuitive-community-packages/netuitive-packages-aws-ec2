{
  "policy": {
    "actions": [
      {
        "category": 3,
        "type": "event"
      }
    ],
    "conditions": [
      {
        "analytic": "actual",
        "level": 95,
        "metric": "aws.ec2.cpuutilization",
        "operator": ">"
      }
    ],
    "deleted": false,
    "description": "The CPU on the EC2 instance has exceeded 95% for at least 15 minutes.",
    "duration": 900,
    "enabled": false,
    "name": "AWS EC2 - CPU Threshold Exceeded",
    "scope": {
      "elementType": "EC2"
    }
  }
}